<div id="loggedinTopbar">
        <div id="accBalanceDiv">
          <span id="accBalance">Account Balance: <span style="font-size: 20px; font-weight: 500;">$<%= (loggedInSeller.accountBalance).toFixed(2) %></span></span>
        </div>
        <div id="addProductDiv">
            <form action="/addnewproduct" style="width: 100%; display: flex; justify-content: center;">
                <button class="btn btn-light btn-sm" id="addProductButton"><svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor" class="bi bi-plus" viewBox="0 0 16 16">
                    <path d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4z"/></svg>
                  Add New Product
                </button>
            </form>
        </div>
        <div id="logOutDiv">
            <form action="/logout" method="post" style="width: 100%; display: flex; justify-content: center;">
                <button class="btn btn-light btn-sm" id="logOutButton">Log Out</button>
            </form>
        </div>
    </div>
</div>
